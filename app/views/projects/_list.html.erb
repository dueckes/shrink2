<ul id="projects">
  <% projects.each do |project| %>
    <% li_for(project, :style => element_style(:hidden => project == hide)) do -%>
      <%= link_to_remote project.name, { :url => project_path(project), :method => :get }, { :id => dom_id(project, :show_link) } %>
    <% end -%>
  <% end -%>
</ul>
