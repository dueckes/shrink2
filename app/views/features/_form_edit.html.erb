<%= error_messages_for :feature %>
<% remote_form_for(:feature, feature, :url => feature_path(feature), :update => dom_id(feature), :html => { :method => :put }, :builder => FeatureFormBuilder) do |feature_form| -%>
  <%= render :partial => "form_common", :locals => { :feature_form => feature_form } %>
  <%= feature_form.submit_button %>&nbsp;<%= feature_form.cancel_button %>
<% end -%>
