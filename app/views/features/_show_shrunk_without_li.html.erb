<% content_tag(:div, :class => "feature-name" ) do -%>
  <%= link_to_remote feature.title, { :update => dom_id(feature), :url => package_feature_path(feature.package, feature), :method => :get }, { :class => "list-title" } %>
  <%= render :partial => "features/show_common", :locals => { :feature => feature } %>
<% end -%>
